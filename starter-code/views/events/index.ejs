<div>
  <div>
    <button type="button" name="notifications">
      <span class="glyphicon glyphicon-bell"></span>
    <!--  <span class="badge badge-light">  countNotifications </span> -->
    </button>

    <% if( user.role == 'ADMIN' ){ %>
      <a href="/event"> Crear Evento </a>
    <% } else {%>

    <% } %>
    <% if (typeof(events) !== 'undefined'){ %>
      <% events.forEach((event) =>{ %>
        <div>
          <div>
            <div id="imgEvent">
                <img src="img/<%= event.imgEvent %>"alt="eventPhoto">
            </div>
            <p> <%= event.nameTour %> </p>
            <p> <%= event.artist %> </p>
            <% if( user.role != 'ADMIN' ){ %>
              <p> <a href="#"> Mas info </a> </p>
            <% } %>
            <div>
              <% if( user.role == 'ADMIN' ){ %>
                <a href="/<%= event._id %>/edit"> Editar </a>
                <a href="/<%= event._id %>/delete"> Eliminar </a>
              <% } %>
            </div>
          </div>
        </div>
      <% }); %>
    <% } %>
  </div>
</div>

